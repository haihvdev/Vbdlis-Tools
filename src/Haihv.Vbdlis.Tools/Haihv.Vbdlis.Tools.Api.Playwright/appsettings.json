{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PlaywrightSettings": {
    "Headless": true,
    "SlowMo": 0,
    "Timeout": 30000,
    "UserDataRoot": "",
    "SessionIdleMinutes": 30
  },
  "VbdlisSettings": {
    "BaseUrl": "https://bgi.mplis.gov.vn/dc",
    "AuthenUrl": "https://authen.mplis.gov.vn/account/login",
    "XuLyCongViec": "XuLyCongViec",
    "CungCapThongTinGCN": "CungCapThongTinGiayChungNhan/Index",
    "QuanLyDieuHanh": "quanlydieuhanh/tongquan",
    "BaoCaoKetQuaTTHCTheoDVHC": "Report/BaoCaoKetQuaGiaiQuyetThuTucHanhChinhTheoDonViHanhChinh"
  }
}
